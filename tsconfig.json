{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "pages/_app.js", "components/Portal.js", "pages/api/nft/[uri].js", "pages/staking/tokens.js", "pages/staking/nfts.js", "pages/index.js", "components/NftCard.js", "components/Nav.js", "components/Modal.jsx"],
  "exclude": ["node_modules"]
}
